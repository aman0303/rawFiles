  "HostedEnvironment": {
    "MsiClientId": "##HOSTED_ENV_MSI_CLIENT_ID##"
  },
  "Logging": {
    "LogLevel": {
      "Default": "Debug",
      "Microsoft.AspNetCore": "Information"
    }
  },
