{
  "Database": {
    "Id": "##DATABASE_ID##",
    "ServiceEndpoint": "##DATABASE_SERVICE_ENDPOINT##",
    "AccountUri": "##DATABASE_ACCOUNT_URI##",
    "DatabaseId": "##DATABASE_DATABASE_ID##",
    "RequestTimeoutInSeconds": 6,
    "MaxConnectionLimit": 50,
    "MaxRetryWaitTimeInSeconds": 30
  },
  "BlobStorageProvider": {
    "Id": "##BLOB_STORAGE_PROVIDER_ID##",
    "BlobStorageSubscription": "##BLOB_STORAGE_SUBSCRIPTION##",
    "BlobStorageResourceGroup": "##BLOB_STORAGE_RESOURCE_GROUP##",
    "BlobStorageAccountName": "##BLOB_STORAGE_ACCOUNT_NAME##",
    "BlobStorageAccountUrl": "##BLOB_STORAGE_ACCOUNT_URL##"
  },
  "SynapseProvider": {
    "SynapseWorkspaceName": "##SYNAPSE_WORKSPACE_NAME##",
    "SynapseStorageAccountName": "##SYNAPSE_STORAGE_ACCOUNT_NAME##",
    "SynapseFileSystemContainerName": "##SYNAPSE_FILE_SYSTEM_CONTAINER_NAME##",
    "SynapseDatabaseName": "##SYNAPSE_DATABASE_NAME##"
  },
  "JobManagement": {
    "QueueConnectionString": "##JOB_MANAGEMENT_QUEUE_CONNECTION_STRING##",
    "ServiceEndpoint": "##JOB_MANAGEMENT_SERVICE_ENDPOINT##",
    "AccountUri": "##JOB_MANAGEMENT_ACCOUNT_URI##",
    "DatabaseName": "##JOB_MANAGEMENT_DATABASE_NAME##",
    "CollectionName": "##JOB_MANAGEMENT_COLLECTION_NAME##",
    "QueueNamePrefix": "##JOB_MANAGEMENT_QUEUE_NAME_PREFIX##",
    "DefaultJobTimeoutHours": "2"
